
<%- include("partials/header"); -%>
    <h1>Home</h1>
    <p> <%= startingContent %> </p>


  <%  posts.forEach(function(post){ %>

    <h1><%=post.title%></h1>
    <p>
    <%=post.postBody.substring(0, 100) + " ..."%>
    <a href="/posts/<%=post.title%>">Read More</a>
    </p>
    <span class="badge">Posted: <%= post.postDate.toDateString() %>, <%= post.postDate.toTimeString() %></span>


    <% }) %>


<%- include("partials/footer"); -%>
